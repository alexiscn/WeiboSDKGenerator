{
    "category": "Statuses", 
    "console": "http://open.weibo.com/tools/console?uri=statuses/user_timeline&httpmethod=GET&", 
    "description": "获取用户发布的微博", 
    "parameters": [
        {
            "optional": true, 
            "type": "int64", 
            "name": "uid", 
            "description": "需要查询的用户ID。"
        }, 
        {
            "optional": true, 
            "type": "string", 
            "name": "screen_name", 
            "description": "需要查询的用户昵称。"
        }, 
        {
            "optional": true, 
            "type": "int64", 
            "name": "since_id", 
            "description": "若指定此参数，则返回ID比since_id大的微博（即比since_id时间晚的微博），默认为0。"
        }, 
        {
            "optional": true, 
            "type": "int64", 
            "name": "max_id", 
            "description": "若指定此参数，则返回ID小于或等于max_id的微博，默认为0。"
        }, 
        {
            "optional": true, 
            "type": "int", 
            "name": "count", 
            "description": "单页返回的记录条数，最大不超过100，超过100以100处理，默认为20。"
        }, 
        {
            "optional": true, 
            "type": "int", 
            "name": "page", 
            "description": "返回结果的页码，默认为1。"
        }, 
        {
            "optional": true, 
            "type": "int", 
            "name": "base_app", 
            "description": "是否只获取当前应用的数据。0为否（所有数据），1为是（仅当前应用），默认为0。"
        }, 
        {
            "optional": true, 
            "type": "int", 
            "name": "feature", 
            "description": "过滤类型ID，0：全部、1：原创、2：图片、3：视频、4：音乐，默认为0。"
        }, 
        {
            "optional": true, 
            "type": "int", 
            "name": "trim_user", 
            "description": "返回值中user字段开关，0：返回完整user字段、1：user字段仅返回user_id，默认为0。"
        }
    ], 
    "url": "https://api.weibo.com/2/statuses/user_timeline.json", 
    "response": [
        {
            "type": "string", 
            "name": "created_at", 
            "description": "微博创建时间"
        }, 
        {
            "type": "int64", 
            "name": "id", 
            "description": "微博ID"
        }, 
        {
            "type": "int64", 
            "name": "mid", 
            "description": "微博MID"
        }, 
        {
            "type": "string", 
            "name": "idstr", 
            "description": "字符串型的微博ID"
        }, 
        {
            "type": "string", 
            "name": "text", 
            "description": "微博信息内容"
        }, 
        {
            "type": "string", 
            "name": "source", 
            "description": "微博来源"
        }, 
        {
            "type": "boolean", 
            "name": "favorited", 
            "description": "是否已收藏，true：是，false：否"
        }, 
        {
            "type": "boolean", 
            "name": "truncated", 
            "description": "是否被截断，true：是，false：否"
        }, 
        {
            "type": "string", 
            "name": "in_reply_to_status_id", 
            "description": "（暂未支持）回复ID"
        }, 
        {
            "type": "string", 
            "name": "in_reply_to_user_id", 
            "description": "（暂未支持）回复人UID"
        }, 
        {
            "type": "string", 
            "name": "in_reply_to_screen_name", 
            "description": "（暂未支持）回复人昵称"
        }, 
        {
            "type": "string", 
            "name": "thumbnail_pic", 
            "description": "缩略图片地址，没有时不返回此字段"
        }, 
        {
            "type": "string", 
            "name": "bmiddle_pic", 
            "description": "中等尺寸图片地址，没有时不返回此字段"
        }, 
        {
            "type": "string", 
            "name": "original_pic", 
            "description": "原始图片地址，没有时不返回此字段"
        }, 
        {
            "type": "object", 
            "name": "geo", 
            "description": "地理信息字段 详细"
        }, 
        {
            "type": "object", 
            "name": "user", 
            "description": "微博作者的用户信息字段 详细"
        }, 
        {
            "type": "object", 
            "name": "retweeted_status", 
            "description": "被转发的原微博信息字段，当该微博为转发微博时返回 详细"
        }, 
        {
            "type": "int", 
            "name": "reposts_count", 
            "description": "转发数"
        }, 
        {
            "type": "int", 
            "name": "comments_count", 
            "description": "评论数"
        }, 
        {
            "type": "int", 
            "name": "attitudes_count", 
            "description": "表态数"
        }, 
        {
            "type": "int", 
            "name": "mlevel", 
            "description": "暂未支持"
        }, 
        {
            "type": "object", 
            "name": "visible", 
            "description": "微博的可见性及指定可见分组信息。该object中type取值，0：普通微博，1：私密微博，3：指定分组微博，4：密友微博；list_id为分组的组号"
        }, 
        {
            "type": "object", 
            "name": "pic_ids", 
            "description": "微博配图ID。多图时返回多图ID，用来拼接图片url。用返回字段thumbnail_pic的地址配上该返回字段的图片ID，即可得到多个图片url。"
        }, 
        {
            "type": "object array", 
            "name": "ad", 
            "description": "微博流内的推广微博ID"
        }
    ], 
    "signature": "UserTimeline", 
    "path": "/2/statuses/user_timeline.json", 
    "method": "GET"
}